-- Carregar Rayfield UI Antigo (Sirius)
local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name = "Brookhaven Hub by Davi",
    LoadingTitle = "Brookhaven Trollz",
    LoadingSubtitle = "Carregando scripts...",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "BrookhavenHub",
        FileName = "Configs"
    },
    KeySystem = false,
})

-- Aba principal
local MainTab = Window:CreateTab("Scripts Brookhaven", 4483362458)

MainTab:CreateButton({
    Name = "Barulhos Horror",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ameicaa1/brookhaven-script/main/brookhaven%20script.lua"))()
    end,
})

MainTab:CreateButton({
    Name = "Rael Hub",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Laelmano24/Rael-Hub/main/main.txt"))()
    end,
})

MainTab:CreateButton({
    Name = "SP Hub",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/as6cd0/SP_Hub/main/Brookhaven"))()
    end,
})

MainTab:CreateButton({
    Name = "Ficar Invis√≠vel",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostPlayer352/Test4/main/Invisible%20Gui"))()
    end,
})

MainTab:CreateButton({
    Name = "Admin Menu",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/BrookhavenDev/BrookhavenScript/main/AdminMenu.lua"))()
    end,
})

MainTab:CreateButton({
    Name = "AstroX Hub",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/AstroxHub/Brookhaven/main/AstroXHub.lua"))()
    end,
